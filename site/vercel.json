{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "hugo --gc --minify",
  "devCommand": "hugo server -D",
  "installCommand": "curl -L https://github.com/gohugoio/hugo/releases/download/v0.147.0/hugo_extended_0.147.0_linux-amd64.tar.gz | tar -xz hugo && mv hugo /usr/local/bin/ && hugo mod tidy",
  "outputDirectory": "public",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/docs",
      "destination": "/en/docs",
      "permanent": false
    },
    {
      "source": "/demo",
      "destination": "/en/demo",
      "permanent": false
    },
    {
      "source": "/blog",
      "destination": "/en/blog",
      "permanent": false
    }
  ]
}
